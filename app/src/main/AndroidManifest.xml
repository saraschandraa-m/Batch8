<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appstone.androidbatch8">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Batch8">
        <activity
            android:name="location.GoogleMapActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="intentactivityresult.TaskEditActivity"
            android:exported="false" />
        <activity
            android:name="intentactivityresult.TaskHomeActivity"
            android:exported="false" />
        <activity
            android:name="intentactivityresult.TaskLoginActivity"
            android:exported="false" />

        <activity
            android:name="toolbarmenu.ToolbarActivity"
            android:exported="false" />
        <activity
            android:name="intent.ImplicitIntentActivity"
            android:exported="false" />
        <activity
            android:name="sharedpref.HomeActivity"
            android:exported="false" />
        <activity
            android:name="sharedpref.LoginActivity"
            android:exported="true" />
        <activity
            android:name="ui.UIActivity"
            android:exported="false" />
        <activity
            android:name=".ThirdActivity"
            android:exported="false" />
        <activity
            android:name=".SecondActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
    </application>

</manifest>